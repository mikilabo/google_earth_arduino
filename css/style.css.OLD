/************************
-----html5------
************************/

#main
{
	border: 1px solid green;
	background:bfcac4;
}
p
{
	padding: 10px 10px 10px 10px;
	color: green;
	font-family:"Courier New";
	font-weight:bold;
	font-size:medium;
	
}

#distance
{
	margin-left:48px;
	width:30px;
	overflow:hidden;
	border: 1px solid orange;
	display:inline;
	padding:2px 2px 2px 2px;
	background-color:f8eedf;
	
	-webkit-box-shadow: 0px 0px 4px #4195fc;
       -moz-box-shadow: 0px 0px 4px #4195fc;
            box-shadow: 0px 0px 4px #4195fc;
}

button
{
	margin:auto;
	height: 25px;
	width:50px;
	background:#2e9497;
    -moz-animation-name: glow;
    -moz-animation-duration: 2s;
    -moz-animation-iteration-count: infinite;
    -webkit-animation-name: glow;
    -webkit-animation-duration: 2s;
    -webkit-animation-iteration-count: infinite;
    background-image: -moz-radial-gradient(50% 110%, circle farthest-corner, #FFF, transparent);
    background-image: -webkit-radial-gradient(50% 110%, circle farthest-corner, #FFF, transparent);
    background-image: -o-radial-gradient(50% 110%, circle farthest-corner,#FFF, transparent);
    background-image: -ms-radial-gradient(50% 110%, circle farthest-corner, #FFF, transparent);
    background-image: radial-gradient(50% 110%, circle farthest-corner);
    
    border-radius: 5px 10px 5px 10px / 10px 5px 10px 5px;
	border-radius: 5px;
	border-radius: 5px 10px / 10px;
	
	-webkit-box-shadow: 0px 0px 4px #4195fc;
    -moz-box-shadow: 0px 0px 4px #4195fc;
    box-shadow: 0px 0px 4px #4195fc;
}

#go
{
	margin-left:35px;
	margin-bottom:10px;
}
#left
{
	margin-left:75px;
	margin-bottom:10px;
}
#back
{
	margin-left:134px;
}

#power
{
	margin-left:20px;
	width:160px;
}
#stop
{
	background:#fe7f11;
}
#amount
{
	color:#777;
	font-size:72px;
	text-align:center;	
}
#slideValue
{
	height: 20px;
	width:50px;
	border:1px solid orange;
	position:fixed;
	
	-webkit-box-shadow: 0px 0px 4px #4195fc;
    -moz-box-shadow: 0px 0px 4px #4195fc;
    box-shadow: 0px 0px 4px #4195fc;

}

#defaultSlider
{
	margin-left:75px;
	-webkit-box-shadow: 0px 0px 4px #4195fc;
    -moz-box-shadow: 0px 0px 4px #4195fc;
    box-shadow: 0px 0px 4px #4195fc;
}


